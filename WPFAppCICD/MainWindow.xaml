<Window x:Class="WPFAppCICD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFAppCICD"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="label1" Content="Version: " HorizontalAlignment="Left" Margin="25,20,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="{Binding AssemblyVersionLabel}" HorizontalAlignment="Left" Margin="85,20,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
