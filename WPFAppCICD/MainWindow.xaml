<Window x:Class="WPFAppCICD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFAppCICD"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="label1" Content="WPFAppCICD: " HorizontalAlignment="Left" Margin="25,20,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="{Binding WPFAppCICDAssemblyVersionLabel}" HorizontalAlignment="Left" Margin="115,20,0,0" VerticalAlignment="Top"/>

        <Label x:Name="label3" Content="SideProject: " HorizontalAlignment="Left" Margin="25,40,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label4" Content="{Binding SideProjectObjectAssemblyVersionLabel}" HorizontalAlignment="Left" Margin="115,40,0,0" VerticalAlignment="Top"/>

        <Label x:Name="label5" Content="Feature Branch: " HorizontalAlignment="Left" Margin="25,60,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
